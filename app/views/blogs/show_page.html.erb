<div ng-controller="BlogController">
<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12">
      <br>
      <ul>
      <h3 class="blog_page_date"><%= @date %></h3>
      <br>
      <br>
        <li ng-repeat="blog in blogs | blogByDate:'<%= @date %>'">
          <p class="blog_page_title">{{blog.title}}</p>
          <pre class="blog_page_description">{{blog.description}}</pre>
        </li>
      </ul>
    </div>
  </div>
</div>





<div class="container-fluid blogs_index">
      <div class="row text-center">
      <h4 class="blog_index_heading">Recent Blogs</h4>
    </div>
    <div class="row text-center">
      <div class="col-xs-12">
        <div ng-repeat="blog in blogs | recentMonths | limitTo:10">
          <a href="/blog_page?date={{blog.date}}">
            <h6 class="archives_index_blog_title">{{blog.title}}</h6>
          </a>
        </div>
      </div>
    </div>
  <div class="row text-center">
